My submission 
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t ; 
	cin>>t;
	while(t--)
	{
	    int A[1001]  = {0};     
	    int max_i = 0 ; 
	    int n ; 
	    cin>>n;
	    int k = n ; 
	    while(n--)
	    {   int x ; cin>>x; 

	        A[x] = A[x] + 1 ;
	        if(A[x] > A[max_i])
	        {
	            max_i = x ; 
	        }
	    }
	    
	    cout<<k-A[max_i]<<endl; 
	    
	}
	
	
	
	return 0;
}
